{
  "name": "@hypnodroid/to-make-sense",
  "version": "1.0.0",
  "description": "Vitest matcher that validates AI responses make sense in production contexts",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest run --bail 1 --pool=forks --poolOptions.forks.singleFork",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit false",
    "deploy": "npm test && npm run deploy:force",
    "deploy:force": "npm run build && npm publish --access public",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vitest",
    "testing",
    "matcher",
    "ai",
    "llm",
    "validation",
    "ollama"
  ],
  "author": "loqwai",
  "license": "MIT",
  "devDependencies": {
    "vitest": "2.1.8",
    "typescript": "5.7.3",
    "@types/node": "22.10.5"
  },
  "peerDependencies": {
    "vitest": ">=0.25.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loqwai/to-make-sense.git"
  },
  "bugs": {
    "url": "https://github.com/loqwai/to-make-sense/issues"
  },
  "homepage": "https://github.com/loqwai/to-make-sense#readme"
}
